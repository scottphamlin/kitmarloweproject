      list {
        (
         ## (The text has been supplied based on evidence internal to this text
         ##                       (context, etc.).) 
         "internal"
         | 
           ## (The text has been supplied based on an external source (which should be
           ##                       referenced through the @source attribute).) 
           "external"
         | 
           ## (The text has been supplied based on guesswork.) 
           "conjecture")+
      }
    }?,
    
    ## one or more words indicating why the text has had to be supplied, e.g. overbinding, faded-ink, lost-folio, omitted-in-original.
    attribute reason {
      list {
        (
         ## (Heavy type or writing on reverse obscuring text.) 
         "bleedthrough"
         | 
           ## (Type apparently malformed or fractured.) 
           "broken-type"
         | 
           ## (Dirt on the page, tearing, etc.) 
           "damage"
         | 
           ## (Misprint or typesetting error.) 
           "error-in-original"
         | 
           ## (The ink has faded, obscuring the text.) 
           "faded-ink"
         | 
           ## (Type not (sufficiently) inked.) 
           "gap-in-inking"
         | 
           ## (Smudging dating from the original print process.) 
           "ink-smudged"
         | 
           ## (The folio is missing from the document.) 
           "lost-folio"
         | 
           ## (Omitted from the original text due to a printing or typesetting
           ##                       error.) 
           "omitted-in-original"
         | 
           ## (The original page has been cut or cropped with the loss of some
           ##                       text.) 
           "original-cropped"
         | 
           ## (The facsimile photograph does not include the whole surface.) 
           "scan-cropped"
         | 
           ## (The facsimile photograph is not clear, out-of-focus, etc.) 
           "scan-unclear"
         | 
           ## (The text is not clear for some reason not covered by other values of
           ##                       @reason.) 
           "unclear")+
      }
    },
    empty
  }
